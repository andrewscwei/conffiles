{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "jsx": "react-jsx",
    "lib": [
      "DOM.Iterable",
      "DOM",
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "outDir": "lib",
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "ESNext"
  },
  "exclude": [
    "node_modules",
    "lib"
  ],
  "include": [
    "src"
  ]
}
